<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="myTestBean" class="org.fade.demo.springframework.beans.parse.MyTestBean">
		<meta key="testStr" value="aaaaaaa"/>
	</bean>

	<bean id="getBeanTest" class="org.fade.demo.springframework.beans.parse.lookupmethod.GetBeanTest">
<!--		<lookup-method name="getBean" bean="teacher" />-->
		<lookup-method name="getBean" bean="student" />
	</bean>

	<bean id="teacher" class="org.fade.demo.springframework.beans.parse.lookupmethod.Teacher" />

	<bean id="student" class="org.fade.demo.springframework.beans.parse.lookupmethod.Student" />

	<bean id="testChangeMethod" class="org.fade.demo.springframework.beans.parse.replacedmethod.TestChangeMethod">
<!--		<replaced-method name="changeMe" replacer="replacer" />-->
		<replaced-method name="changeMe" replacer="replacerAlias" />
	</bean>

	<bean id="replacer" class="org.fade.demo.springframework.beans.parse.replacedmethod.TestMethodReplacer" />

	<alias name="replacer" alias="replacerAlias" />

</beans>